{
    "name": "test/package-name",
    "description": "some description",
    "keywords": [
        "test"
    ],
    "homepage": "https://github.com/test/package-name",
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Test\\Url\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Url\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true
    }
}
